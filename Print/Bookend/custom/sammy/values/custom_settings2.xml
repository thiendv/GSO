<?xml version="1.0" encoding="utf-8"?>
<resources>
    
    <!-- カスタム名 -->
    <string name="custom_name">sammy</string>
    
    <!-- サービスバインド用カテゴリー名 ※マニフェストは参照が効かないので修正すること -->
