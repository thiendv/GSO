    <!-- Splash画像の下の背景を白にするかどうかフラグ = 0:そのまま(黒), 1:白 -->
    <string name="splash_background_white">1</string>
    
    <!-- InAppBilling base64EncodedPublicKey 
    	アプリを公開するGoogleアカウントの公開鍵を指定する必要があります　-->
